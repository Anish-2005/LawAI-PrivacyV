# Environment Variables Template
# Copy this file to .env and fill in your actual values
# NEVER commit the actual .env file to git!

# Django Settings
SECRET_KEY=your-secret-key-here-generate-a-new-one
DEBUG=False
ALLOWED_HOSTS=your-domain.com,your-vercel-app.vercel.app

# Database Configuration
DB_NAME=your_database_name
DB_USER=your_database_user
DB_PASSWORD=your_database_password
DB_HOST=your_database_host
DB_PORT=5432

# Production Database (Railway/Vercel)
# DB_NAME=railway
# DB_USER=postgres
# DB_PASSWORD=your_actual_password
# DB_HOST=junction.proxy.rlwy.net
# DB_PORT=11448

# Security Settings
CORS_ALLOWED_ORIGINS=https://your-frontend-domain.com,https://your-app.vercel.app
